(define (problem mrmh_planning)
    (:domain mrmh_planning)
    (:objects l1  l2  l3  l4  l5  l6  l7  l8  l9  - location
     worker1  r1  - agent
     t3l4  t3l7  t3l9  - task)
    (:init
        (= (travel_dist) 0)
        (path l1 l2) (path l2 l1)
        (path l1 l4) (path l4 l1)
        (path l2 l3) (path l3 l2)
        (path l2 l5) (path l5 l2)
        (path l3 l6) (path l6 l3)
        (path l4 l5) (path l5 l4)
        (path l4 l7) (path l7 l4)
        (path l5 l6) (path l6 l5)
        (path l5 l8) (path l8 l5)
        (path l6 l9) (path l9 l6)
        (path l7 l8) (path l8 l7)
        (path l8 l9) (path l9 l8)
  (empty l1) (empty l2) (empty l3) (empty l4) (empty l5) (empty l6) (empty l7) (empty l8) (empty l9)   (task_loc t3l4 l4)
  (task_loc t3l7 l7)
  (task_loc t3l9 l9)
        (agent_at worker1  l1 ) (agent_at r1  l1 )
  (= (p_success worker1 t3l4) 0.99)
  (= (p_success worker1 t3l7) 0.99)
  (= (p_success worker1 t3l9) 0.99)
  (= (p_success r1 t3l4) 0.97)
  (= (p_success r1 t3l7) 0.97)
  (= (p_success r1 t3l9) 0.97)
 
    )
    (:goal (and 
      (task_done t3l4)
  (task_done t3l7)
  (task_done t3l9)
))
    (:metric minimize (travel_dist))
)